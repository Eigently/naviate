{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "lib": ["es6"],
    "target": "es5",
    "outDir": "dist",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "strictPropertyInitialization": false
  },
  "include": ["src/**/*.ts"]
}
